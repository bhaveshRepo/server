generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model User {
  id        Int               @id @default(autoincrement())
  U_Id      String            @unique
  username  String
  questions Questions[]
}

model Questions {
  id       Int        @id @default(autoincrement())
  Q_Id     String     @unique
  question String
  options  String
  answer   String
  user     User @relation(fields: [UserId], references: [U_Id])
  UserId   String
}
